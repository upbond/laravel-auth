{
    "name": "upbond/laravel-auth",
    "description": "Laravel plugin that helps authenticate with the upbond auth service",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "upbond/auth-php": "^1.0.0",
        "illuminate/support": "5.* | ^6.0 | ^7.0",
        "illuminate/contracts": "5.* | ^6.0 | ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7|^8|^9",
        "squizlabs/php_codesniffer": "^3.2",
        "phpcompatibility/php-compatibility": "^8.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0"
    },
    "scripts": {
        "test": "SHELL_INTERACTIVE=1 \"vendor/bin/phpunit\" --coverage-text ",
        "phpcs": "\"vendor/bin/phpcs\"",
        "sniffs": "\"vendor/bin/phpcs\" -e"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/facade"
        ],
        "psr-0": {
            "Upbond\\Auth\\Login\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Upbond\\Auth\\Login\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Upbond\\Auth\\Login\\LoginServiceProvider"
            ]
        }
    }
}
